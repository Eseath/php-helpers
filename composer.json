{
    "name": "eseath/helpers",
    "type": "library",
    "version": "0.3.0",
    "license": "MIT",
    "description": "Collection of helper functions written in PHP.",
    "homepage": "https://github.com/eseath/php-helpers",
    "keywords": [
        "helpers",
        "functions"
    ],
    "require": {
        "php": ">=8.0",
        "ext-fileinfo": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Eseath\\Helpers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Eseath\\Helpers\\Tests\\": "tests/"
        }
    }
}
